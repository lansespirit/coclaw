---
import BaseLayout from '../layouts/BaseLayout.astro';
import Section from '../components/layout/Section.astro';
import QuickInstallTabs from '../components/QuickInstallTabs.tsx';

// SEO-optimized structured data
const canonicalURL = new URL(Astro.url.pathname, Astro.site);
const structuredData = {
  '@context': 'https://schema.org',
  '@type': 'HowTo',
  name: 'How to Install OpenClaw - AI Assistant Setup Guide',
  description:
    'Complete guide to installing OpenClaw AI assistant on macOS, Linux, Windows, or cloud platforms. Get started in 5 minutes.',
  step: [
    {
      '@type': 'HowToStep',
      name: 'Choose Installation Method',
      text: 'Select between local installation (macOS, Linux, Windows) or cloud deployment (VPS, AWS, Docker)',
    },
    {
      '@type': 'HowToStep',
      name: 'Install Node.js',
      text: 'Install Node.js 22+ (Node 22 LTS recommended)',
    },
    {
      '@type': 'HowToStep',
      name: 'Install OpenClaw',
      text: 'Run npm install -g openclaw@latest (or pnpm add -g openclaw@latest)',
    },
    {
      '@type': 'HowToStep',
      name: 'Run the Setup Wizard',
      text: 'Run openclaw onboard (recommended: openclaw onboard --install-daemon)',
    },
    {
      '@type': 'HowToStep',
      name: 'Open the Dashboard',
      text: 'Open http://127.0.0.1:18789/ or run openclaw dashboard',
    },
  ],
};

const breadcrumbData = {
  '@context': 'https://schema.org',
  '@type': 'BreadcrumbList',
  itemListElement: [
    { '@type': 'ListItem', position: 1, name: 'Home', item: 'https://coclaw.com/' },
    { '@type': 'ListItem', position: 2, name: 'Getting Started', item: canonicalURL.toString() },
  ],
};
---

<BaseLayout
  title="OpenClaw Installation Guide - Install AI Assistant on macOS, Linux, Windows & Cloud"
  description="Complete OpenClaw installation guide for all platforms. Install on macOS, Linux, Windows, Docker, VPS, AWS, and more. Get your AI assistant running in 5 minutes with step-by-step instructions."
  keywords="OpenClaw installation, install OpenClaw, OpenClaw setup guide, OpenClaw macOS install, OpenClaw Linux installation, OpenClaw Windows setup, OpenClaw Docker, OpenClaw VPS deployment, OpenClaw AWS, AI assistant installation, OpenClaw tutorial, OpenClaw quick start"
>
  <script type="application/ld+json" slot="head" set:html={JSON.stringify(structuredData)} />
  <script type="application/ld+json" slot="head" set:html={JSON.stringify(breadcrumbData)} />

  <div
    class="bg-background min-h-screen overflow-x-hidden relative selection:bg-pink-500/30 selection:text-pink-950 dark:selection:text-pink-200"
  >
    <!-- Hero Section -->
    <Section containerClass="text-center pt-24 pb-8">
      <h1
        class="text-5xl md:text-6xl font-bold mb-6 bg-gradient-to-br from-foreground to-foreground/70 bg-clip-text text-transparent"
      >
        Getting Started with OpenClaw
      </h1>
      <p class="text-xl text-default-800 dark:text-default-600 max-w-3xl mx-auto">
        Choose your installation path and get your personal AI assistant running in minutes
      </p>
    </Section>

    <!-- Quick Installation Guide -->
    <Section containerClass="pt-8 pb-12">
      <div class="max-w-5xl mx-auto">
        <div class="mb-8 text-center">
          <h2 class="text-3xl md:text-4xl font-bold mb-4">
            How to Install OpenClaw: Quick Setup Guide
          </h2>
          <p class="text-lg text-default-700 dark:text-default-500">
            Install OpenClaw AI assistant in 5 minutes on macOS, Linux, Windows, or Docker
          </p>
        </div>

        <QuickInstallTabs client:load />
      </div>
    </Section>

    <!-- Installation Path Selector -->
    <Section>
      <div class="mb-12 text-center">
        <h2 class="text-3xl md:text-4xl font-bold mb-4">
          Choose Your OpenClaw Installation Method
        </h2>
        <p class="text-lg text-default-700 dark:text-default-500 max-w-3xl mx-auto">
          Select the best installation option for your needs: local setup for privacy or cloud
          deployment for 24/7 availability
        </p>
      </div>
      <div class="grid md:grid-cols-2 gap-8 max-w-5xl mx-auto">
        <!-- Local Installation Card -->
        <div
          class="group relative overflow-hidden rounded-2xl border-2 border-divider bg-content1 p-8 hover:border-primary/50 transition-all duration-300 hover:shadow-xl hover:shadow-primary/10"
        >
          <div class="mb-6">
            <div
              class="w-16 h-16 rounded-2xl bg-gradient-to-br from-blue-500 to-cyan-600 flex items-center justify-center mb-4"
            >
              <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M9.75 17L9 20l-1 1h8l-1-1-.75-3M3 13h18M5 17h14a2 2 0 002-2V5a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
                ></path>
              </svg>
            </div>
            <h3 class="text-3xl font-bold mb-3 group-hover:text-primary transition-colors">
              Install OpenClaw Locally
