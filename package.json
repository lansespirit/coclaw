{
  "name": "coclaw",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,ts,tsx,astro,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,cjs,ts,tsx,astro,json,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@heroui/react": "^2.8.8",
    "@heroui/theme": "^2.4.26",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.17.1",
    "framer-motion": "^12.29.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "husky": "^9.1.7",
    "pagefind": "^1.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1"
  }
